http://localhost:3000 {
	# sets up "http://localhost:3000/somepath" to hit localhost:5174 with "/somepath" stripped from what sveltekit receives
	handle_path /somepath/* {
		reverse_proxy localhost:5174
	}

	# sets up "http://localhost:3000/otherpath" to hit localhost:5175 with "/otherpath" stripped from what sveltekit receives
	handle_path /otherpath/* {
		reverse_proxy localhost:5175
	}

	reverse_proxy localhost:5173
}
